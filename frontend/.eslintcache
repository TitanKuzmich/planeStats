  const getLibCount = () => {\n    return libItems.reduce((qty, item) => Number(item.qty) + qty, 0);\n  };\n\n  const getLibSubTotal = () => {\n    return libItems\n      .reduce((item) => item.qty, 0)\n      .toFixed(2);\n  };\n\n  return (\n    <>\n      <div className=\"cartscreen\">\n        <div className=\"cartscreen__left\">\n          <h2>Библиотека</h2>\n\n          {libItems.length === 0 ? (\n            <div>\n               В вашей библиотеке еще нет моделей самолетов. <Link to=\"/\">Вернуться на главную</Link>\n            </div>\n          ) : (\n            libItems.map((item) => (\n              <LibItem\n                key={item.product}\n                item={item}\n                qtyChangeHandler={qtyChangeHandler}\n                removeHandler={removeFromLibHandler}\n              />\n            ))\n          )}\n        </div>\n\n        <div className=\"cartscreen__right\">\n          <div className=\"cartscreen__info\">\n            <p>Всего моделей в библиотеке ({getLibCount()})</p>\n            <p>${getLibSubTotal()}</p>\n          </div>\n          <div>\n            <button onClick={onRemoveAllHandle}>Удалить все модели</button>\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default LibScreen;\n","/Users/aleksandr/Desktop/tutorials/mern-apps/planes/planeStats/frontend/src/screens/PlaneScreen.jsx",["81","82","83","84"],"/Users/aleksandr/Desktop/tutorials/mern-apps/planes/planeStats/frontend/src/components/Plane.jsx",["85"],"import \"./Product.css\";\nimport {Link} from \"react-router-dom\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport {removeFromCatalog} from \"../redux/actions/planeActions\";\n\nconst Plane = ({imageUrl, description, name, productId}) => {\n\n    const dispatch = useDispatch();\n    const onRemoveFromCatalog = (id) => {\n        dispatch(removeFromCatalog(id))\n    }\n\n    return (\n        <div className=\"product\">\n            <img src={imageUrl} alt={name}/>\n\n  